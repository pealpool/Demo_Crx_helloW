{
    "manifest_version": 2,
    "name": "THE ONE 外贸运营助手",
    "version": "0.0.0.1",
    "description": "专注外贸运营工具开发",
    "icons": {
        "16": "images/logoTO.png",
        "48": "images/logoTO.png",
        "128": "images/logoTO.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/logoTO.png",
            "38": "images/logoTO.png"
        },
        "default_title": "THE ONE 外贸运营助手",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/jquery-3.5.1.min.js", "js/background.js"],
        "persistent": false
    },
    "permissions": [
        "*://*.alibaba.com/*",
        "tabs",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.alibaba.com/*"
            ],
            "js": ["js/jquery-3.5.1.min.js", "js/content-script.js"],
            "run_at": "document_start"
        }
    ]
}